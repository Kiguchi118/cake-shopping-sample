<h1>ユーザー情報</h1>
<table>
    <thead>
        <tr>
            <th>name</th>
            <th>email</th>
            <th>退会</th>
        </tr>
    </thead>
    <tbody>
        <% @users.each do |user| %>
        <tr>
            <td>
                <%= link_to admins_user_path(user) do %>
                    <%= user.name %>
                <% end %>
            </td>
            <td><%= user.email%></td>
            <td><% if user.deleted_at? %>退会済み<% end %></td>
        </tr>
        <% end %>
    </tbody>
</table>
